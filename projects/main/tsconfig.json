{
  "extends": "@mfx-js/tsconfig",
  "compilerOptions": {
    "types": ["node", "react", "@mfx-js/tsconfig"],
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "@mfx-js/core": ["../../packages/mfx-core/src"],
      "@mfx-js/core/*": ["../../packages/mfx-core/src/*"],
      "@mfx-js/framework": ["../../packages/mfx-framework/src"],
      "@mfx-js/player": ["../../packages/mfx-player/src"],
      "@mfx-js/player/*": ["../../packages/mfx-player/src/*"],
      "@mfx-js/three": ["../../packages/mfx-player/src"],
      "@mfx-js/three/*": ["../../packages/mfx-player/src/*"],
    },
  },
  "exclude": ["mfx-config.js"]
}
