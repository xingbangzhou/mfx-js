{
  "name": "@mfx-js/framework",
  "version": "1.2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "keywords": [
    "mfx",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xingbangzhou/mfx-js.git",
    "directory": "packages/mfx-framework"
  },
  "maintainers": [
    "xingbangzhou"
  ],
  "scripts": {
    "build": "pnpm prebuild && pnpm build:legacy && pnpm build:modern && pnpm build:node && pnpm build:stable && pnpm build:types && pnpm build:copy-files",
    "build:node": "node ../../scripts/build.mjs node",
    "build:modern": "node ../../scripts/build.mjs modern",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:legacy": "node ../../scripts/build.mjs legacy",
    "build:types": "node ../../scripts/buildTypes.mjs",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "prebuild": "rimraf dist tsconfig.build.tsbuildinfo",
    "release": "pnpm build && pnpm publish dist --access public --no-git-checks"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "@mfx-js/core": ">=1.1.0"
  },
  "devDependencies": {
    "@mfx-js/tsconfig": "workspace:^",
    "@mfx-js/core": "workspace:^"
  }
}
