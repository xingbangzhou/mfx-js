{
  "name": "@mfx0/framework",
  "version": "1.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "keywords": [
    "mfx",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xingbangzhou/mfx.git",
    "directory": "packages/mfx-framework"
  },
  "scripts": {
    "build": "yarn build:legacy && yarn build:modern && yarn build:node && yarn build:stable && yarn run build:types && yarn run build:copy-files",
    "build:node": "node ../../scripts/build.mjs node",
    "build:modern": "node ../../scripts/build.mjs modern",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:legacy": "node ../../scripts/build.mjs legacy",
    "build:types": "node ../../scripts/buildTypes.mjs",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo",
    "release": "yarn build && npm publish build --access public"
  },
  "dependencies": {
    "@mfx0/base": "^1.0.1",
    "@mfx0/utils": "^1.0.1"
  },
  "peerDependencies": {
    "@mfx0/base": ">=1.0.1"
  },
  "devDependencies": {
    "@mfx0/tsconfig": "^1.0.0"
  }
}
