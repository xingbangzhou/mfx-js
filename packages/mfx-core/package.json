{
  "name": "@mfx-js/core",
  "version": "1.0.0-beta.0",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "keywords": [
    "mfx",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xingbangzhou/mfx.git",
    "directory": "packages/mfx-core"
  },
  "scripts": {
    "build": "yarn run prebuild && yarn build:node && yarn build:modern && yarn build:stable && yarn build:legacy && yarn run build:types && yarn run build:copy-files",
    "build:node": "node ../../scripts/build.mjs node",
    "build:modern": "node ../../scripts/build.mjs modern",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:legacy": "node ../../scripts/build.mjs legacy",
    "build:types": "node ../../scripts/buildTypes.mjs",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo",
    "release": "yarn build && npm publish build --access public"
  },
  "dependencies": {
  },
  "peerDependencies": {
  },
  "devDependencies": {
    "@mfx-js/tsconfig": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
