{
  "name": "@mfx-js/core",
  "version": "1.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "keywords": [
    "mfx",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xingbangzhou/mfx-js.git",
    "directory": "packages/mfx-core"
  },
  "maintainers": [
    "xingbangzhou"
  ],
  "scripts": {
    "build": "pnpm prebuild && pnpm build:node && pnpm build:modern && pnpm build:stable && pnpm build:legacy && pnpm build:types && pnpm build:copy-files",
    "build:node": "node ../../scripts/build.mjs node",
    "build:modern": "node ../../scripts/build.mjs modern",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:legacy": "node ../../scripts/build.mjs legacy",
    "build:types": "node ../../scripts/buildTypes.mjs",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo",
    "release": "pnpm build && npm publish build --access public"
  },
  "devDependencies": {
    "@mfx-js/tsconfig": "workspace:^"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
